CURDIR=`dirname $0`
. ${CURDIR}/base.profile

# 是否使用内网IP 
# 如果false，则使用公网IP，OKCHAIN_TESTNET_FULL_HOSTS设置其他节点公网IP
# 如果是true，则必须设置IP_PREFIX，OKCHAIN_TESTNET_FULL_HOSTS设置其他节点内网IP
IP_INNET=true
IP_PREFIX=192.168
HOSTS_PREFIX=okchain
SCP_TAG="root"
USER=root

OKCHAIN_TESTNET_SEED_NODES=("192.168.80.178")
OKCHAIN_TESTNET_VAL_NODES=("192.168.80.188" "192.168.80.191" "192.168.80.192" "192.168.80.172")
OKCHAIN_TESTNET_FULL_NODES=("192.168.80.168" "192.168.80.145")