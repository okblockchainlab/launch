OKCHAIN_TESTNET_SEED_NODES=("192.168.13.116")
OKCHAIN_TESTNET_VAL_NODES=("192.168.13.121" "192.168.13.122" "192.168.13.123" "192.168.13.124")
OKCHAIN_TESTNET_FULL_NODES=("192.168.13.125")